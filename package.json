{
  "name": "realtime-notes-backend",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "vercel-build": "prisma generate && tsc",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "@prisma/client": "*",
    "bcrypt": "*",
    "cors": "*",
    "dotenv": "*",
    "express": "*",
    "helmet": "*",
    "jsonwebtoken": "*",
    "ws": "*"
  },
  "devDependencies": {
    "@types/bcrypt": "*",
    "@types/cors": "^2.8.19",
    "@types/express": "*",
    "@types/jest": "*",
    "@types/jsonwebtoken": "*",
    "@types/node": "*",
    "@types/supertest": "^6.0.3",
    "@types/ws": "*",
    "jest": "*",
    "prisma": "*",
    "supertest": "*",
    "ts-jest": "*",
    "ts-node-dev": "*",
    "typescript": "*"
  }
}
